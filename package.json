{
  "name": "blog",
  "version": "1.0.0",
  "author": "reco_luan",
  "scripts": {
    "dev": "vuepress dev . --open --host \"localhost\"",
    "build": "vuepress build .",
    "decrypt": "encrypt decrypt --source-dir blogs/Git --key-file keys.json --temp .temp-encrypt",
    "encrypt": "encrypt encrypt --source-dir blogs/Git --key-file keys.json --temp .temp-encrypt",
    "check": "encrypt check --source-dir blogs/Git --key-file keys.json --temp .temp-encrypt"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check"
    }
  },
  "devDependencies": {
    "@oak-tree-house/vuepress-plugin-encrypt": "^1.0.1",
    "husky": "^7.0.1",
    "markdown-it-disable-url-encode": "^1.0.1",
    "vuepress": "^1.8.2",
    "vuepress-plugin-boxx": "0.0.7",
    "vuepress-plugin-one-click-copy": "^1.0.5",
    "vuepress-plugin-robots": "^1.0.1",
    "vuepress-plugin-sakura": "^1.1.0",
    "vuepress-theme-reco": "1.6.6"
  },
  "description": "",
  "dependencies": {
    "vuepress-plugin-comment": "^0.7.3"
  }
}
